citHeader("To cite package 'FSAdata' in publications use:")

desc <- packageDescription("FSAdata")
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", desc$Date)
vers <- paste("R package version", desc$Version)
citEntry(entry="Manual",
         title = "FSAdata: Fisheries Stock Analysis, Datasets",
         author = personList(as.person("Derek H. Ogle")),
         year = year,
         note = vers,
         textVersion = paste("Ogle, D.H. ",year,". FSAdata: Fisheries Stock Analysis, Datasets. ",vers,".", sep="")
)
